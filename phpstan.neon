parameters:
    level: max
    paths:
        - src
    checkMissingIterableValueType: false
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Constant SIGTERM not found#'
        - '#Method [^\s]* has no return typehint specified.#'
        - '#Method [^\s]* has parameter \$[^\s]* with no typehint specified.#'
